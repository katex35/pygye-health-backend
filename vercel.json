{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "functions": {
        "app.py": {
            "excludeFiles": ".next/**\n*.cache/**\nnode_modules/**\npublic/**\napp/**\ntrain_model.py"
        }
    }
} 